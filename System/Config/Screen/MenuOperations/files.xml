<operationsMenuFiles>
      <Include includeFlag = "0x0040"
               filename="$CONFIG_DIR/Screen/MenuOperations/packVerify.xml" />
      
      <Screen commandId = "orderStatus" 
              count = "1"
              screenClass = "Gui.FlexScreen.FleXmlScreen">
        <Params>
          <param name = "xmlFileName" 
                 value = "MenuOperations\orderStatus.xml" />
          <param name = "xmlSource" 
                 value = "project" />
        </Params>
      </Screen>
      
      <Screen commandId = "containerRouting" 
              count = "1"
              screenClass = "Gui.ContainerRouting.FrmContainerRouting">
        <Params>
          <param name = "connectionId" 
                 value = "default" />
          <param name = "relayCommands" 
                 value = "refresh:routeStatus" />
          <param name = "title" 
                 value = "Carton Routing" />
        </Params>
      </Screen>

      <Screen commandId = "routeStatus" 
              count = "1"
              screenClass = "Gui.ContainerRouting.FrmRouteStatus">
        <Params>
          <param name = "connectionId" 
                 value = "default" />
          <param name = "editable" 
                 value = "true" />
          <param name = "relayCommands" 
                 value = "refresh:containerRouting" />
        </Params>
      </Screen>
      
      <Screen commandId="wavePlanner" 
              count="1"
              screenClass="Gui.WavePlanner.FrmWaveScheduler">
        <Params>
          <param name="connectionId" 
                 value="default" />
          <param name="title" 
                 value="Wave Planner" />
          <param name="xmlSource" 
                 value="project" />
          <param name="xmlFileName" 
                 value="wavePlanner.xml" />
          <param name="size" 
                 value="650, 585" />
          <param name="prepareWave" 
                 value="true" />
          <param name="reHandler" 
                 value="Gui.WavePlanner.StandardGuiREHandler" />
        </Params>
      </Screen>
      
      <Screen commandId = "templateDesigner"
              count = "1"
              screenClass = "com.qcsoftware.ui.matrixcontainer.FrmMatrixDesigner">
        <Params>
          <param name = "connectionId" 
                 value = "default" />
          <param name = "defaultLength" 
                 value = "36.0" />
          <param name = "defaultWidth" 
                 value = "18.0" />
          <param name = "defaultHeight" 
                 value = "12.0" />
          <param name = "defaultWeight" 
                 value = "100.0" />
          <param name = "defaultDimensionUom" 
                 value = "in" />
          <param name = "defaultWeightUom" 
                 value = "lb" />
          <param name = "cellResizeEnabled" 
                 value = "true" />
        </Params>

        <WEIGHT_UNITS>
          <UNIT id = "lb" 
                value = "pound" />
          <UNIT id = "kg" 
                value = "kilogram" />
        </WEIGHT_UNITS>

        <DIMENSIONAL_UNITS>
          <UNIT id = "in" 
                value = "inch" />
          <UNIT id = "mm" 
                value = "millimeter" />
          <UNIT id = "cm" 
                value = "centimeter" />
        </DIMENSIONAL_UNITS>
      </Screen>

      <Screen commandId = "templateAssignment"
              count = "1"
              screenClass = "com.qcsoftware.ui.matrixcontainer.FrmMatrixAssignTemplate">
        <Params>
          <param name = "connectionId" 
                 value = "default" />
        </Params>
      </Screen>
      
      <Screen commandId = "locationPlans"
              count = "1"
              screenClass = "com.qcsoftware.ui.fulfillmentmgr.FrmLocationZonePlans">
        <Params>
          <param name = "connectionId" 
                 value = "default" />
        </Params>
      </Screen>
      
      <Screen commandId = "matrixPickPut"
              count = "1"
              screenClass = "com.qcsoftware.ui.matrixcontainer.FrmSimplePickPut">
        <Params>
          <param name = "connectionId" value = "default" />
        </Params>
      </Screen>
      
      <Screen commandId = "locationPlans"
              count = "1"
              screenClass = "com.qcsoftware.ui.fulfillmentmgr.FrmLocationZonePlans">
        <Params>
          <param name = "connectionId" value = "default" />
        </Params>
      </Screen>
</operationsMenuFiles>
