<FLEXSCREEN title = "Chart Wizard" 
            height = "700" 
            width = "1200" 
            frametype = "internal"
            report = "true">

  <!-- a dbConnect here is a universal connect object  -->
  <DBCONNECT type = "predefined" 
             connectionId = "default" 
             setglobal = "true"/>

  <FLEXTABLECOMPONENT name = "ClusterOrderCountsTable"
                      init = "desc" 
                      refresh = "true" 
                      records = "100"
                      greenbar = "true">
    <DBCONNECT type = "global" />
    <TABLE key = "desc" >
      <SELECT sql = "select 'Prepared' 'Status', 'Test1' 'TEST', 100 'OrderCount', 100 'OrderCount2'
                      union all
                      select 'Planned' 'Status', 'Test2' 'TEST', 150 'OrderCount', 100 'OrderCount2'
                      union all
                      select 'Picking' 'Status', 'Test3' 'TEST', 200 'OrderCount', 100 'OrderCount2'
                      union all
                      select 'Picked' 'Status', 'Test4' 'TEST', 100 'OrderCount', 100 'OrderCount2'"/>
    </TABLE>
  </FLEXTABLECOMPONENT>
</FLEXSCREEN>
